<h1>Search!</h1>

<nav class ="users-search">
  <label>Search: <input type="text" name="query[username]" value=""></label>
  <ul>
    <% @users.each do |user| %>
      <li>
        <a href="<%= user_url(user) %>"><%= user.username %></a>
        <%= render 'follows/form', user: user %>
      </li>
    <% end %>
  </ul>
</nav>
